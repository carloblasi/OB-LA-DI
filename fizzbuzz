INT a, mul_3, mul_5;

LET a = 1;

WHILE a <= 10 [

	LET mul_3 = a;
	LET mul_5 = a;

	WHILE mul_3 > 0 [
		LET mul_3 = mul_3 - 3;
	] 
	WHILE mul_5 > 0 [
		LET mul_5 = mul_5 - 5;
	] 

	IF mul_3 = 0 [

		IF mul_5 = 0 [
			PRINT "FizzBuzz";
		]
		ELSE [
			PRINT "Fizz";
		]
	] 
	ELSE [
		IF mul_5 = 0 [

			IF mul_3 = 0 [
				PRINT "FizzBuzz";
			]
			ELSE [
				PRINT "Buzz";
			]
		] 
		ELSE [
			PRINT a;
		]
	]
	LET a = a + 1;
]
